@model dynamic

@{
	ViewBag.Title = "title";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Run load</h2>

<div class="row">
	<div class="label">Domain:</div>
	<div class="input large"><input class="large" type="text" id="loadrun_domain"/></div>
</div>
<div class="row">
	<div class="label">Duration:</div>
	<div class="input small"><input class="small" type="text" id="loadrun_duration"/> sec</div>
</div>
<div class="row">
	<div class="label">RampUp Period:</div>
	<div class="input"><input class="small" type="text" id="loadrun_rampup"/> sec</div>
</div>
<div class="row actions">
	<input type="button" class="btn btn_primary" value="Run"/>
</div>